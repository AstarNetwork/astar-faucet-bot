{
    "discord": {
        "scope": ["bot", "applications.commands"],
        "permissions": 2147551232,
        "slashCommands": [
            {
                "name": "drip",
                "type": 1,
                "description": "Receive a testnet token from the faucet",
                "options": [
                    {
                        "name": "network",
                        "description": "The name of the testnet",
                        "type": 3,
                        "required": true,
                        "choices": [
                            {
                                "name": "Shibuya",
                                "value": "shibuya"
                            }
                        ]
                    },
                    {
                        "name": "address",
                        "description": "Your SS58 (Substrate) or H160 (EVM) public address for receiving the token",
                        "type": 3,
                        "required": true
                    }
                ]
            }
        ]
    },

    "network": {
        "shibuya": {
            "endpoint": "wss://rpc.shibuya.astar.network",
            "amount": 15,
            "requestTimeout": 30,
            "types": {
                "Keys": "AccountId",
                "SmartContract": {
                    "_enum": {
                        "Evm": "H160",
                        "Wasm": "AccountId"
                    }
                },
                "EraStakingPoints": {
                    "total": "Balance",
                    "stakers": "BTreeMap<AccountId, Balance>",
                    "formerStakedEra": "EraIndex",
                    "claimedRewards": "Balance"
                },
                "EraRewardAndStake": {
                    "rewards": "Balance",
                    "staked": "Balance"
                }
            }
        },
        "shiden": {
            "endpoint": "wss://rpc.shiden.astar.network",
            "amount": 0.002,
            "requestTimeout": 1440,
            "types": {
                "Keys": "AccountId",
                "SmartContract": {
                    "_enum": {
                        "Evm": "H160",
                        "Wasm": "AccountId"
                    }
                },
                "EraStakingPoints": {
                    "total": "Balance",
                    "stakers": "BTreeMap<AccountId, Balance>",
                    "formerStakedEra": "EraIndex",
                    "claimedRewards": "Balance"
                },
                "EraRewardAndStake": {
                    "rewards": "Balance",
                    "staked": "Balance"
                }
            }
        },
        "astar": {
            "endpoint": "wss://rpc.astar.network",
            "amount": 0.002,
            "requestTimeout": 1440,
            "types": {
                "Keys": "AccountId",
                "SmartContract": {
                    "_enum": {
                        "Evm": "H160",
                        "Wasm": "AccountId"
                    }
                },
                "EraStakingPoints": {
                    "total": "Balance",
                    "stakers": "BTreeMap<AccountId, Balance>",
                    "formerStakedEra": "EraIndex",
                    "claimedRewards": "Balance"
                },
                "EraRewardAndStake": {
                    "rewards": "Balance",
                    "staked": "Balance"
                }
            }
        }
    }
}
